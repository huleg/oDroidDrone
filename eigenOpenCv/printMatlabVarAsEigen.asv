function [ output_args ] = printMatlabVarAsEigen( typeEigen, nameEigen, varMatlab )
%PRINTMATLABVARASEIGEN Summary of this function goes here
%   Detailed explanation goes here

if ~isempty(typeEigen)
    fprintf( '%s %s(%d, %d);\n', typeEigen, nameEigen, size(varMatlab,1), size(varMatlab,2) );
end
fprintf( '%s << ', nameEigen );
fprintf( '%e', varMatlab(1) );
for i=2:length( varMatlab(:) )
fprintf( ', %e', varMatlab(i) );
end
fprintf( ';' );
